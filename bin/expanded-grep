#!/usr/bin/env perl
use strict;

sub expand_regexp($)
{
    my ($skeleton) = @_;
    my @skeleton = ($skeleton =~ m/./g);
    $skeleton = join("[^/]*", @skeleton);
    $skeleton = qr([^/]*$skeleton[^/]*)i;
    return $skeleton;
}

@ARGV = map {expand_regexp $_} @ARGV;
while (<STDIN>) {
    my $matched = 1;
    for my $arg (@ARGV) {
        if ($_ !~ m/$arg/) {
            $matched = 0;
            last;
        }
    }
    if ($matched) {
        print $_;
    }
}
