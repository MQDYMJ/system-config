#!/usr/bin/env perl

use strict;

my ($v1, $v2) = @ARGV;

$v1 =~ s/[^0-9_.]//;
$v2 =~ s/[^0-9_.]//;
my @v1 = split(/\.|_/, $v1);
my @v2 = split(/\.|_/, $v2);

while (@v1) {
    $v1 = int shift @v1;
    $v2 = int shift @v2;

    if ($v1 < $v2) {
        print "<";
        exit;
    } elsif ($v1 > $v2) {
        print ">";
        exit;
    }
}

if (@v2) {
    print "<";
} else {
    print "=";
}
