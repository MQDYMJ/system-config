#!/bin/bash
set -e
my-adb -r stop

if test -z "$ANDROID_SERIAL"; then
    die "Can't do it without ANDROID_SERIAL set"
fi

if test "$ANDROID_SERIAL" = a2024058; then
    die "Must not run it on a2024058"
fi

my-adb 'cd /data;
        for x in *; do
            if test $x = media -o $x = app -o $x = debian -o $x = misc -o $x = property; then
                continue;
            else
                rm -r $x;
            fi;
        done
        cd /data/property;
        for x in *; do
            if test $x = persist.sys.usb.config; then
                continue
            fi
            rm -r $x
        done
        cd /data/misc/;
        for x in *; do
            if test $x = adb -o $x = wifi; then
                continue;
            else
                rm -r $x;
            fi;
        done
        cd /data/misc/wifi
        for x in *; do
            if test "$x" = wpa_supplicant.conf; then
                continue
            else
                rm -r $x
            fi
        done
'
