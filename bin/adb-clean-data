#!/bin/bash
my-adb -r stop

my-adb 'cd /data;
        for x in *; do
            if test $x = media -o $x = debian -o $x = misc; then
                continue;
            else
                rm -r $x;
            fi;
        done
        cd /data/misc/;
        for x in *; do
            if test $x = adb; then
                continue;
            else
                rm -r $x;
            fi;
        done'
