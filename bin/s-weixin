#!/bin/bash
x=$(select-output-line -o '
    (
        cd ~/system-config/bin;
        for x in $(ls -l |grep emacs-weixin.sh | pn 9) t1-putclip; do
            echo $x;
        done|sort -u
        echo do-work
        echo adb-browse-with-pc
        echo find-or-exec T1Wrench T1Wrench
    ) | uniq-even-non-ajacent '
)

$x "$@"&
