#!/bin/bash

if test -z "$USER"; then
    USER=$(whoami)
fi

if test "$USER" = bhj; then
   true;
   exit
fi

if test -e ~/.ssh/id_rsa-baohaojun.pub; then
    true
    exit
fi

if test "$(basename $0)" = no-if-not-bhj; then
    false
    exit
fi

if yes-or-no-p "$@"; then
    true
    exit
fi

false
