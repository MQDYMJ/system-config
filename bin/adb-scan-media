#!/usr/bin/env bash

## start code-generator "^\\s *#\\s *"
# generate-getopt qquiet
## end code-generator
## start generated code
TEMP=$(getopt -o qh \
              --long quiet,help,no-quiet \
              -n $(basename -- $0) -- "$@")
quiet=false
eval set -- "$TEMP"
while true; do
    case "$1" in

        -q|--quiet|--no-quiet)
            if test "$1" = --no-quiet; then
                quiet=false
            else
                quiet=true
            fi
            shift
            ;;
        -h|--help)
            set +x
            echo
            echo
            echo Options and arguments:
            printf %06s '-q, '
            printf %-24s '--[no-]quiet'
            echo
            exit
            shift
            ;;
        --)
            shift
            break
            ;;
        *)
            die "internal error: $(. bt; echo; bt | indent-stdin)"
            ;;
    esac
done


## end generated code

if test "$quiet" = true; then
    exec >/dev/null 2>&1
fi

for x in "$@"; do
    my-adb am startservice --user 0 -n com.bhj.setclip/.PutClipService --es picture "$x"
done
