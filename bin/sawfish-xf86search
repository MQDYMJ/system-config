#!/bin/bash

s-dicts "$(getclip-android)"
exit

top_window=$(sawfish-top-window)


case "${top_window}" in
    Wrench)
        s_how=button3 s
        ;;
    Emacs)
        sawfish-send-focused-window-event XF86Search
        ;;
    XTerm)
        sawfish-send-focused-window-event F2
        ;;
    *)
        find-or-exec Search beagle-search
        ;;
esac
