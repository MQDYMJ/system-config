#!/bin/bash

set -e
if test ! -d .repo; then
    die "must run at top of repo"
fi
rm -rf .repo/repo-external

mkdir -p .repo/repo-external

my-rfa ap | (
    last=;
    while read d; do
        if [[ $d =~ $last/.* ]]; then
            continue;
        fi;
        echo $d;
        last=$d;
    done ) | xargs mv-with-dir-struct .repo/repo-external
hint "external: $(echo *)"
rm * -rf
mv .repo/repo-external/* .
