#!/bin/bash
IFS=$'\n'

PATH=$(string-join : $(
           echo "${PATH}" | tr : '\n' | grep -v brew
                   ))
if test "$#" = 0; then
    if test "$BASH_SOURCE" = "$0"; then
        # we are being run
        start_recursive_shell nobrew
    else
        # we are being sourced.
        return 0
    fi
else
    "$@"
fi
