#!/bin/bash
host=localhost
user=ant

if test "$ANT_DEBUG_BUILD" = true; then
    exit 0
fi

if test -e ./.ant-get-keystore-passwd; then
    . ./.ant-get-keystore-passwd
elif test -e ~/.ant-get-keystore-passwd; then
    . ~/.ant-get-keystore-passwd
else
    pass=$(get-about-me keystore-passwd)
    if test "$pass" != unknown; then
        echo "$pass"
        exit
    fi
fi

get-authinfo "$host" "$user"
