<?xml version="1.0" encoding="utf-8"?>
<QueryMapping xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <Mappings>
    <Mapping QueryKeyword="artist" BeagrepProperty="fixme:artist" Tokenize="true">Artist of the music</Mapping>
    <Mapping QueryKeyword="mailto" BeagrepProperty="parent:fixme:to_name" Tokenize="true" />
    <Mapping QueryKeyword="mailto" BeagrepProperty="fixme:to_name" Tokenize="true">Name of receipient</Mapping>
    <Mapping QueryKeyword="author" BeagrepProperty="dc:author" Tokenize="true">Author of the content</Mapping>
    <Mapping QueryKeyword="summary" BeagrepProperty="dc:subject" Tokenize="true">Brief description of the content</Mapping>
    <Mapping QueryKeyword="imagemodel" BeagrepProperty="exif:Model" Tokenize="true">Camera model as specified in exif or IPTC tags</Mapping>
    <Mapping QueryKeyword="imagecomment" BeagrepProperty="iptc:caption" Tokenize="true">IPTC caption</Mapping>
    <Mapping QueryKeyword="imagecomment" BeagrepProperty="exif:UserComment" Tokenize="true">Exif comments</Mapping>
    <Mapping QueryKeyword="imagecomment" BeagrepProperty="jfif:Comment" Tokenize="true">JFIF comments</Mapping>
    <Mapping QueryKeyword="imagecomment" BeagrepProperty="digikam:caption" Tokenize="true">Digikam User comments</Mapping>
    <Mapping QueryKeyword="imagecomment" BeagrepProperty="fspot:Description" Tokenize="true">F-Spot User comments</Mapping>
    <Mapping QueryKeyword="speakingto" BeagrepProperty="fixme:speakingto" Tokenize="false">Person engaged in conversation</Mapping>
    <Mapping QueryKeyword="mailtoaddr" BeagrepProperty="parent:fixme:to_address" Tokenize="true" />
    <Mapping QueryKeyword="mailtoaddr" BeagrepProperty="fixme:to_address" Tokenize="true">Email address of receipient</Mapping>
    <Mapping QueryKeyword="inattachment" BeagrepProperty="parent:fixme:hasAttachments" Tokenize="false">Use 'inattachment:true' for email attachments.</Mapping>
    <Mapping QueryKeyword="imagetag" BeagrepProperty="image:tag" Tokenize="true">FSpot, Digikam image tags</Mapping>
    <Mapping QueryKeyword="source" BeagrepProperty="beagrep:Source" Tokenize="false">Name of the backend</Mapping>
    <Mapping QueryKeyword="creator" BeagrepProperty="dc:creator" Tokenize="true">Creator of the content</Mapping>
    <Mapping QueryKeyword="emblem" BeagrepProperty="nautilus:emblem" Tokenize="false">Use emblem to identify items tagged with a specific emblem in nautilus.</Mapping>
    <Mapping QueryKeyword="mailinglist" BeagrepProperty="parent:fixme:mlist" Tokenize="true" />
    <Mapping QueryKeyword="mailinglist" BeagrepProperty="fixme:mlist" Tokenize="true">Mailing list id e.g. dashboard-hackers.gnome.org</Mapping>
    <Mapping QueryKeyword="mailfrom" BeagrepProperty="parent:fixme:from_name" Tokenize="true" />
    <Mapping QueryKeyword="mailfrom" BeagrepProperty="fixme:from_name" Tokenize="true">Name of email sender</Mapping>
    <Mapping QueryKeyword="genre" BeagrepProperty="fixme:genre" Tokenize="false">Genre of the music</Mapping>
    <Mapping QueryKeyword="ext" BeagrepProperty="beagrep:FilenameExtension" Tokenize="false">File extension, e.g. ext:jpeg. Use ext: to search in files with no extension.</Mapping>
    <Mapping QueryKeyword="filetype" BeagrepProperty="beagrep:FileType" Tokenize="false">Type of content for HitType File</Mapping>
    <Mapping QueryKeyword="email" BeagrepProperty="vCard:EMAIL" Tokenize="true">Email in KAddressbook</Mapping>
    <Mapping QueryKeyword="host" BeagrepProperty="fixme:host" Tokenize="true">The host of this entitiy.</Mapping>
    <Mapping QueryKeyword="mimetype" BeagrepProperty="beagrep:MimeType" Tokenize="false">Mimetype of the content</Mapping>
    <Mapping QueryKeyword="notetag" BeagrepProperty="note:tag" Tokenize="true">Tag associated with a tomboy note.</Mapping>
    <Mapping QueryKeyword="type" BeagrepProperty="beagrep:HitType" Tokenize="false">Hittype of the content e.g. File, IMLog, MailMessage</Mapping>
    <Mapping QueryKeyword="title" BeagrepProperty="dc:title" Tokenize="true">Title</Mapping>
    <Mapping QueryKeyword="mailfromaddr" BeagrepProperty="parent:fixme:from_address" Tokenize="true" />
    <Mapping QueryKeyword="mailfromaddr" BeagrepProperty="fixme:from_address" Tokenize="true">Email address of sender</Mapping>
    <Mapping QueryKeyword="pkggroup" BeagrepProperty="pkg:group" Tokenize="true">Group to which the package belongs e.g. System/Configuration/Packaging</Mapping>
    <Mapping QueryKeyword="inarchive" BeagrepProperty="fixme:inside_archive" Tokenize="false">Use 'inarchive:true' for files inside an archive.</Mapping>
    <Mapping QueryKeyword="album" BeagrepProperty="fixme:album" Tokenize="true">Album name of the music</Mapping>
    <Mapping QueryKeyword="filename" BeagrepProperty="beagrep:NoPunctFilename" Tokenize="true">File name</Mapping>
    <Mapping QueryKeyword="filename" BeagrepProperty="beagrep:SplitFilename" Tokenize="true">File name</Mapping>
  </Mappings>
</QueryMapping>
